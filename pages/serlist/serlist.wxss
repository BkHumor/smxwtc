.index_list {
  color: #666;
  padding-top: 3rpx;
  background-color: #efeff4;
}

.index_list .index_list_nav {
  background: #fff;
  margin-bottom: 30rpx;
  padding: 40rpx 40rpx;
  overflow: hidden;
  border-bottom: 1px solid #ededee;
  border-top: 1px solid #ededee;
}

.list_header_left image {
  width: 80rpx;
  height: 80rpx;
  margin-right: 10rpx;
  float: left;
  border-radius: 40rpx;
  overflow: hidden;
}

.list_header view {
  float: left;
  line-height: 80rpx;
  color: #1e1e1e;
  font-size: 16px;
}

.list_header  .list_header_right {
  float: right;
  color: #999;
  font-size: 12px;
}

.list_header {
  padding-bottom: 18rpx;
  overflow: hidden;
}

.listPic {
  width: 50%;
  height: 360rpx;
  margin-bottom: 16rpx;
}

.listTitle {
  font-size: 18px;
  color: #1e1e1e;
  margin-bottom: 16rpx;
}

.listContent {
  line-height: 19px;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  color: #646464;
  font-size: 16px;
  margin-bottom: 16rpx;
  max-height: 36px;
}
.list_footer {
  overflow: hidden;
  height:120rpx;
  line-height:190rpx;
  margin-top: -8px;
}
  
 .list_footer .h_canvas{
  float: right;
  padding-left: 44rpx;
  padding-right: 16rpx;
  margin-top:4px;
  background-size: 44rpx auto;
  color: #999;
}   
.agree_1, .commNum,.agree_0 {
  float:right;
  
}
.commNum {
  margin-right : 20px;
}
.agree_1 image, .commNum image {
  width: 20px;
  height: 20px;
}
.agree_0 image {
   width: 20px;
  height: 20px;
}
/* canvas */
.myCanvas{
  background-color: transparent;
  width: 90px;
  height:400px;
  /* display: none; */
}

.v-canvas{
    position: fixed;
    right: 20px;
    bottom: 60px;
    width: 90px;
    background-color: transparent;
}

.v-img{
  position: fixed;
  right: 30px;
  bottom: 30px;
}

.bt-img{
  width: 40px;
  height: 40px;
  transform: scale(1);
  -webkit-transform: scale(1);
  -webkit-transition:ease all;
  -moz-transition:ease all;
  transition:ease all;
  -webkit-transition-duration:700ms;
  -moz-transition-duration:700ms;
  transition-duration:700ms;
  
}
.addr_icon{
   width: 23rpx;
  height: 23rpx;
  vertical-align: middle;
  }
.addr_info{
  line-height: 24rpx;
  height: 24rpx;
  margin-left: 10rpx;
  font-family:"PingFangSC-Regular","sans-serif","STHeitiSC-Light","微软雅黑","Microsoft YaHei";
  font-size: 24rpx;
  color:lightblue;
}
.hidden{
  display: none;
}
.ser-add {
position: fixed;
bottom: 0;
width: 100%;
}