page {

  background-color:rgb(229, 229, 229);

}
.index_list {
  color: #666;
  padding-top: 3rpx;
  background-color: #efeff4;
}
.list_header_left image {
  width: 59rpx;
  height: 60rpx;
  margin-right: 10rpx;
  float: left;
  border-radius: 40rpx;
  overflow: hidden;
}
.text-info{
  color:#117887;
}
.list_header_left .nickname {
display:inline-block;
line-height:55rpx;
font-family:"PingFangSC-Regular","sans-serif","STHeitiSC-Light","微软雅黑","Microsoft YaHei";
font-size:22rpx;
color:rgb(178, 178, 178);
}
.index_list .index_list_nav {
  background: #fff;
  padding: 10rpx 20rpx;
  overflow: hidden;
  border-bottom: 1px solid #ededee;
  border-top: 1px solid #ededee;
}

.list_header_left image {
  width: 80rpx;
  height: 80rpx;
  margin-right: 10rpx;
  float: left;
  border-radius: 40rpx;
  overflow: hidden;
}

.list_header view {
  float: left;
  line-height: 80rpx;
  color: #1e1e1e;
  font-size: 26rpx;
}

.list_header  .list_header_right {
  float: right;
  color: #999;
  font-size: 12px;
}

.list_header {
  padding-bottom: 18rpx;
  overflow: hidden;
}

.listPic {
  width: 50%;
  height: 360rpx;
  margin-bottom: 16rpx;
}

.listTitle {
  font-size: 18px;
  color: #1e1e1e;
  margin-bottom: 16rpx;
}

.listContent {
  line-height: 19px;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  color: #646464;
  font-size: 26rpx;
  margin-bottom: 16rpx;
  max-height: 36px;
}
.tipbox{
  padding-top:240rpx;
}
.tipimg{
  text-align: center;
}
.tipimg image{
  width: 200rpx;
  height: 200rpx;
  display: inline-block;
  margin-left:28rpx;

}
